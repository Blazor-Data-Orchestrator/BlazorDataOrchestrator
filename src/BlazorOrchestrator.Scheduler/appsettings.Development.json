{
  "ConnectionStrings": {
    "AzureStorage": "UseDevelopmentStorage=true"
  },
  "SchedulerSettings": {
    "PollingIntervalSeconds": 30,
    "VerboseLogging": true,
    "StuckJobTimeoutHours": 24,
    "RetryCount": 3,
    "RetryDelaySeconds": 5
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
